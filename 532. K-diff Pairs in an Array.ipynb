   "source": [
    "import collections\n",
    "\n",
    "def kpairs(nums,k):\n",
    "    hash=collections.Counter(nums)\n",
    "    result=0\n",
    "    for x in hash:\n",
    "        if(k>0 and k+x in hash):\n",
    "            result+=1\n",
    "        if(k==0 and hash[x]>1):\n",
    "            result+=1\n",
    "    return result\n",
    "\n",
    "\n",
    "\n",
    "kpairs([1,2,4,4,3,3,0,9,2,3],3)"
   ]
