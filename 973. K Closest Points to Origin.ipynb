   "source": [
    "import math\n",
    "points = [[3,3],[5,-1],[-2,4]]\n",
    "k = 2\n",
    "\n",
    "def sol(points,k):\n",
    "    hash=[]\n",
    "    for i in points:\n",
    "        hash.append(  ((i[0],i[1]),math.sqrt( (i[0])**2 +(i[1])**2)))\n",
    "\n",
    "    return [key[0] for key in sorted(hash,key=lambda x : x[1])[:k]]\n",
    "    # print(hash.items())\n",
    "\n",
    "\n",
    "sol(points,k)"
   ]
