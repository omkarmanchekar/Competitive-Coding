    "arr = [3,1,2,4]\n",
    "\n",
    "\n",
    "def sol(arr):\n",
    "    sum=0\n",
    "    for i in range(len(arr)):\n",
    "        for j in range(i+1,len(arr)+1):\n",
    "            sum+=min(arr[i:j])\n",
    "\n",
    "    return sum\n",
    "sol(arr)"
   ]
